{
  "name": "vue3-nuxt3-radix-tailwind-boilerplate",
  "description": "Vue3 + Nuxt3 + Radix + Tailwind + TypeScript + ESlint + Stylelint + Prettier Template",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "type": "vue-tsc --noEmit",
    "prettier-check": "prettier . --check",
    "prettier-fix": "prettier --write --list-different .",
    "lint-check": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint-fix": "eslint --fix --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "css-check": "stylelint \"**/*.{css,vue}\" ",
    "css-fix": "stylelint \"**/*.{css,vue}\" --fix",
    "format": "pnpm prettier-fix && pnpm lint-fix && pnpm css-fix && pnpm type",
    "bootstrap": "pnpm install && pnpm format"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "packageManager": "pnpm@9.11.0",
  "dependencies": {
    "@nuxt/image": "1.8.0",
    "@nuxtjs/google-fonts": "3.2.0",
    "@pinia/nuxt": "0.5.4",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "dayjs": "1.11.13",
    "lucide-vue-next": "0.445.0",
    "nuxt": "3.13.2",
    "nuxt-security": "latest",
    "pinia": "2.2.2",
    "radix-vue": "1.9.6",
    "shadcn-nuxt": "latest",
    "tailwind-merge": "2.5.2",
    "tailwindcss-animate": "1.0.7",
    "tilt-3d": "1.0.4",
    "vue": "3.5.7",
    "vue-i18n": "10.0.1",
    "vue-router": "4.4.5"
  },
  "devDependencies": {
    "@iconify-json/radix-icons": "1.2.0",
    "@iconify/vue": "4.1.2",
    "@intlify/unplugin-vue-i18n": "5.0.0",
    "@nuxtjs/color-mode": "3.5.1",
    "@nuxtjs/eslint-config-typescript": "12.1.0",
    "@pinia-plugin-persistedstate/nuxt": "1.2.1",
    "@tailwindcss/forms": "0.5.9",
    "@tailwindcss/typography": "0.5.15",
    "@typescript-eslint/eslint-plugin": "8.6.0",
    "@typescript-eslint/parser": "8.6.0",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-promise": "7.1.0",
    "eslint-plugin-vue": "9.28.0",
    "pinia-plugin-persistedstate": "4.0.2",
    "postcss": "8.4.47",
    "postcss-html": "1.7.0",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-tailwindcss": "0.6.6",
    "stylelint": "16.9.0",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-standard": "36.0.1",
    "tailwind-scrollbar": "3.1.0",
    "tailwindcss": "3.4.12",
    "tailwindcss-3d": "1.0.7",
    "tailwindcss-radix": "3.0.5",
    "typescript": "5.6.2",
    "vue-tsc": "2.1.6"
  }
}
